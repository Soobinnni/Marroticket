package com.marroticket.user.play.controller;

import org.springframework.stereotype.Controller;
import org.springframework.web.bind.annotation.GetMapping;
import org.springframework.web.bind.annotation.RequestMapping;

@Controller
@RequestMapping("/play")
public class PlayController {
	
	//현재 상연 페이지
	@GetMapping("/playCurrentList")
	public String playCurrentList() {
		return "user/play/playCurrentList";
	}
	
	//지난 상연 페이지
	@GetMapping("/quickReserve")
	public String quickReserve() {
		return "user/play/quickReserve";
	}
	
	//상연 예정 페이지
	@GetMapping("/quickReserve")
	public String quickReserve() {
		return "user/play/quickReserve";
	}
	
	
	//빠른 예매 페이지
	@GetMapping("/quickReserve")
	public String quickReserve() {
		return "user/play/quickReserve";
	}
}
