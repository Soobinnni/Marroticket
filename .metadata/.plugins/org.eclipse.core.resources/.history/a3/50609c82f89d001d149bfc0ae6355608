	<select id="playCurrentList" resultMap="play">
		<![CDATA[ select
		p_poster, 
		p_name, 
		p_theaterName, 
		p_startDate, 
		p_closeDate, 
		p_runningtime 
		from Play 
		where p_closeDate >= sysdate and sysdate >= p_startDate
		order by p_startDate ]]>
	</select>

	<select id="playPassList" resultMap="play">
		<![CDATA[
		SELECT p_number,
		 p_poster,
		p_name,
		p_theaterName,
		p_startDate,
		p_closeDate,
		p_runningtime
		FROM Play
		WHERE sysdate >= p_closeDate
		order
		by
		p_startDate
		]]>
	</select>

	<select id="playExpectedList" resultMap="play">
		<![CDATA[
		SELECT p_number,
		p_poster,
		p_name,
		p_theaterName,
		p_startDate,
		p_closeDate,
		p_runningtime
		FROM Play
		WHERE p_startDate >= sysdate
		order
		by
		p_startDate
		]]>
	</select>

