package com.marroticket.umember.member.service;

import java.util.HashMap;
import java.util.Map;

import org.springframework.beans.factory.annotation.Autowired;
import org.springframework.stereotype.Service;

import com.marroticket.umember.member.domain.UmemberVO;
import com.marroticket.umember.member.mapper.UmemberMapper;

@Service
public class UmemberServiceImpl implements UmemberService {
	@Autowired
	private UmemberMapper umembermapper;
	
	@Override
	public String findId(UmemberVO umember) {
		Map<String, String> map = new HashMap<>();
		map.put("uName", uName);
		map.put("uPhoneNumber", uPhoneNumber);
	
		return umembermapper.findId(map);
	}
	
}
