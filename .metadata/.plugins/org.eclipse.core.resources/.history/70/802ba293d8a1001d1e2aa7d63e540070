package com.marroticket.common.util.Repository;

import org.springframework.data.jpa.repository.JpaRepository;

import com.marroticket.umember.play.domain.PlayVO;

public interface FileRepository extends JpaRepository<PlayVO, String> {
	PlayVO findByptheaterMapFileName(String ptheaterMapFileName);
	PlayVO findBypposterFileName(String pposterFileName);
}	
